{
  "name": "third",
  "author": "erriy <erriy@msn.com>",
  "version": "0.0.5",
  "license": "MIT",
  "homepage": "https://github.com/Erriy/third#readme",
  "bugs": {
    "url": "https://github.com/Erriy/third/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Erriy/third.git"
  },
  "bin": "bin/cli.js",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:staged": "lint-staged",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "test": "mocha"
  },
  "devDependencies": {
    "@commitlint/config-angular": "^12.1.4",
    "@types/mocha": "^8.2.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.2.4",
    "commitlint": "^12.1.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.28.0",
    "eslint-plugin-mocha": "^9.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^9.0.0",
    "prettier": "^2.3.1",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
    "commit-msg": "commitlint -E GIT_PARAMS",
    "pre-commit": "npm run lint:staged",
    "post-commit": "node script/post-commit.js"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{css,json,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "clipboardy": "^2.3.0",
    "command-exists": "^1.2.9",
    "commander": "^7.2.0",
    "concat-stream": "^2.0.0",
    "default-gateway": "^6.0.3",
    "exit-hook": "^2.2.1",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "fuzzy": "^0.1.3",
    "gpg": "^0.6.0",
    "inquirer": "^6.5.2",
    "inquirer-autocomplete-prompt": "^1.4.0",
    "ip": "^1.1.5",
    "multicast-dns": "^7.2.3",
    "openpgp": "5.0.0-3",
    "random-string": "^0.2.0",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2",
    "url-join": "^4.0.1",
    "uuid": "^3.4.0"
  }
}
