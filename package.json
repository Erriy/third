{
  "name": "third",
  "version": "0.2.0",
  "private": true,
  "author": {
    "name": "erriy",
    "email": "erriy@msn.com"
  },
  "build": {
    "appId": "net.on1y.third",
    "productName": "third",
    "artifactName": "${productName}-${version}_${os}_${arch}.${ext}",
    "publish": [
      "github"
    ],
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "target": {
        "arch": [
          "x64",
          "arm64"
        ],
        "target": "default"
      }
    },
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "runAfterFinish": true
    },
    "files": [
      "app",
      "lib"
    ]
  },
  "scripts": {
    "electron:serve": "cross-env DEBUG=true electron ./app/main.js",
    "electron:build": "electron-builder",
    "build": "electron-builder",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:staged": "lint-staged",
    "test": "mocha"
  },
  "main": "app/main.js",
  "dependencies": {
    "axios": "^0.21.1",
    "commander": "^8.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "default-gateway": "^6.0.3",
    "electron-log": "^4.4.1",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.9",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "gpg": "^0.6.0",
    "multicast-dns": "^7.2.3",
    "node-schedule": "^2.0.0",
    "openpgp": "5.0.0-3",
    "random-string": "^0.2.0",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2",
    "url-join": "^4.0.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/config-angular": "^12.1.4",
    "@types/mocha": "^8.2.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.2.4",
    "commitlint": "^12.1.4",
    "cross-env": "^7.0.3",
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "eslint": "^7.28.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^11.0.0",
    "mocha": "^9.0.0",
    "prettier": "^2.3.1",
    "yorkie": "^2.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/Erriy/third/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
    "commit-msg": "commitlint -E GIT_PARAMS",
    "pre-commit": "npm run lint:staged",
    "pre-push": "node script/pre-push.js"
  },
  "homepage": "https://github.com/Erriy/third#readme",
  "license": "MIT",
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{css,json,md}": "prettier --write"
  },
  "readme": "ERROR: No README data found!",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Erriy/third.git"
  }
}
